# In your .cshrc or .login file, define magphys as the directory which
# contains the MAGPHYS programs (setenv magphys /full_path_to_MAGPHYS_directory)
#
# binary files containing filter responses and models
#
setenv MAGPHYS    $HOME/raid/mass/magphys
setenv FILTERS    $MAGPHYS/filters/FILTERBIN.RES
setenv OPTILIB    $MAGPHYS/libs/OptiLIB_cb07.bin
setenv OPTILIBIS  $MAGPHYS/libs/OptiLIBis_cb07.bin
setenv IRLIB      $MAGPHYS/libs/InfraredLIB.bin

# user input files [this can be changed by user]
#
setenv USER_FILTERS   $MAGPHYS/eg_user_files/filters.dat
setenv USER_OBS       $MAGPHYS/eg_user_files/observations.dat


